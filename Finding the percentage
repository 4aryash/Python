#https://www.hackerrank.com/challenges/finding-the-percentage/problem

if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
average()

def average():
    if query_name == name:
        result= (student_marks[0]+student_marks[1]+student_marks[2])/3
        return result
    else:
        return none
