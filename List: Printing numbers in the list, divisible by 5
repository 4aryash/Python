listX=[]                                           
length=int(input("Enter the length of the List:")) 

print("Enter the entries: ")                       

def charlie():
    for i in range(length):
        entries=int(input())
        listX.append(entries)
    print(listX, end="")
    
    for i in range(length):
        if (listX[i]%5) == 0: 
            print(" In the list, the numbers divisible by 5 is:", listX[i])
        else:
            return 0
charlie()                                          
