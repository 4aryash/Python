#https://www.hackerrank.com/challenges/string-validators/problem?h_r=next-challenge&h_v=zen

def alphaNumeric():
    if (s.isalnum() == True):
        print("True")
    alphabet()

def alphabet():
    if (s.isalpha() == True):
        print("True")
    digit()

def digit():
    if (s.isdigit() == True):
        print("True")
    isLower()

def isLower():
    if (s.islower() == True):
        print("True")
    isUpper()
    
def isUpper():
    if (s.isupper() == True):
        print("True")

if __name__ == '__main__':
    s = input()
    alphaNumeric()

