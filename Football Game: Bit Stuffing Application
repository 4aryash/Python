def Dangerous():
    for i in india:
        if india[i+1]==(india[i+2] and india[i+3] and india[i+4] and india[i+5] and india[i+6]):
            break
        else:
            return NotDangerous()
    print("Dangerous")
    i=i+1
    
def NotDangerous():
    print("Not Dangerous.")
    
india= list(map(int,input()))
Dangerous()
