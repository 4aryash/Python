import math
import os
import random
import re
import sys

def reverseArray(arr):
    return (str(arr)[-2:1:-1])      #[start:stop:step]

if __name__ == '__main__':
    arr_count = int(input())
    arr = list(map(int, input().rstrip().split()))
    strRev = reverseArray(arr)
    print(strRev)
