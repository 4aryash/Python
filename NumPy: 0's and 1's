import numpy

n = int(input())

def numpyZeros(int):
    for i in range(n):
        a= numpy.zeros((n,n), dtype= int)
        print(a)
        
def numpyOnes(int):
    for j in range(n):
        b= numpy.ones((n,n), dtype= int)
        print(b)

numpyZeros(int)
numpyOnes(int)


